@model IEnumerable<UserInformation>

<h2>
    User List
</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (UserInformation r in Model)
        {
            <tr>
                <td scope="row">@r.FName</td>
                <td>@r.LName</td>
                <td>@r.Email</td>
                <td >@r.Phone</td>

            </tr>
        }
    </tbody>
</table>